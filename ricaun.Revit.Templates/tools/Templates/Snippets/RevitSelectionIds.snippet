<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Revit Selection ElementId to Element</Title>
            <Author>ricaun</Author>
            <Description>Selection ElementId to Element</Description>
            <Shortcut>rsids</Shortcut>
        </Header>
        <Snippet>
            <Code Language="CSharp"><![CDATA[var $VarName$ = $VarSelection$.GetElementIds().Select(id => $VarDocument$.GetElement(id));]]>
            </Code>
            <Declarations>
                <Literal>
                    <ID>VarDocument</ID>
                    <Default>document</Default>
                    <ToolTip>Document</ToolTip>
                </Literal>
                <Literal>
                    <ID>VarName</ID>
                    <Default>elements</Default>
                </Literal>
                <Literal>
                    <ID>VarSelection</ID>
                    <Default>selection</Default>
                    <ToolTip>Selection</ToolTip>
                </Literal>
            </Declarations>
            <Imports>
                <Import>
                    <Namespace>Autodesk.Revit.DB</Namespace>
                </Import>
                <Import>
                    <Namespace>Autodesk.Revit.UI</Namespace>
                </Import>
                <Import>
                    <Namespace>System.Linq</Namespace>
                </Import>
                <Import>
                    <Namespace>System.Collections.Generic</Namespace>
                </Import>
            </Imports>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
